<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="#242424"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clickable="true"
    android:minWidth="25px"
    android:minHeight="25px">
    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:theme="@style/ToolBarColor"
        android:minHeight="?attr/actionBarSize"
        app:navigationIcon="@drawable/logo"
        android:background="#333333"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <de.hdodenhof.circleimageview.CircleImageView
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:id="@+id/circleImageView1"
        android:src="@drawable/guitarist"
        android:layout_gravity="center"
        android:layout_marginTop="10dp"
        app:civ_border_color="#fff2854b"
        app:civ_border_width="2dp"
        app:civ_fill_color="#00000000" />
    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ratingBar1"
        android:numStars="5"
        android:clickable="true"
        android:stepSize="0.5"
        android:layout_gravity="center" />
    <TextView
        android:text="Alex Lifeson"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView1"
        android:gravity="center"
        android:textSize="25sp"
        android:textStyle="bold"
        android:typeface="serif"
        android:textColor="#ffffff" />
    <TextView
        android:text="Electric Guitar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView10"
        android:gravity="center"
        android:textColor="#f2854b"
        android:textSize="16sp"
        android:textStyle="bold"
        android:typeface="serif" />
    <ScrollView
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/scrollViewProfile">
        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:stretchColumns="1">
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="24.5dp"
                android:id="@+id/linearLayout1"
                android:background="#333333"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp">
                <TextView
                    android:text="Email:"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:id="@+id/textView2"
                    android:textColor="#f2854b"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    android:typeface="serif"
                    android:layout_marginLeft="10dp" />
                <TextView
                    android:text="alex@aol.com"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:id="@+id/emailView"
                    android:gravity="center"
                    android:layout_marginLeft="10dp"
                    android:textColor="#ffffff"
                    android:textSize="16sp"
                    android:typeface="serif" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearLayout4"
                android:background="#333333"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp">
                <TextView
                    android:text="About Me:"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:id="@+id/textView8"
                    android:layout_marginLeft="10dp"
                    android:textColor="#f2854b"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    android:typeface="serif" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearLayout5"
                android:background="#333333"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp">
                <TextView
                    android:text="Alexandar Zivojinovich, OC (born 27 August 1953), better known by his stage name Alex Lifeson, is a Canadian musician, best known as the guitarist of the Canadian rock band Rush. In 1968, Lifeson co-founded the band that would become Rush, with drummer John Rutsey and bassist and singer Jeff Jones. Jones was replaced by Geddy Lee a month later, and Rutsey was replaced by Neil Peart in 1974; the band's line-up has remained the same ever since. "
                    android:id="@+id/aboutView"
                    android:textColor="#ffffff"
                    android:textSize="16sp"
                    android:typeface="serif"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dp" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="48.5dp"
                android:id="@+id/linearLayout6"
                android:gravity="center"
                android:layout_marginTop="10dp">
                <Button
                    android:text="Contact This Musician"
                    android:layout_width="143.5dp"
                    android:layout_height="match_parent"
                    android:id="@+id/btnSearch"
                    android:background="@drawable/Login_Style_Button"
                    android:textColor="#ffffff"
                    android:textStyle="bold"
                    android:typeface="serif"
                    android:gravity="center"
                    android:layout_marginRight="10dp" />
                <Button
                    android:text="Back to Search Results"
                    android:layout_width="143.5dp"
                    android:layout_height="match_parent"
                    android:id="@+id/btnBackToSearch"
                    android:background="@drawable/Login_Style_Button"
                    android:textColor="#ffffff"
                    android:textStyle="bold"
                    android:typeface="serif"
                    android:gravity="center"
                    android:layout_marginLeft="10dp" />
            </LinearLayout>
        </TableLayout>
    </ScrollView>
</LinearLayout>