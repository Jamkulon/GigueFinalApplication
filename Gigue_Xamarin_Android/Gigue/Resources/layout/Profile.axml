<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="#242424"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:descendantFocusability="beforeDescendants"
    android:focusableInTouchMode="true"
    
    android:clickable="true"
    android:minWidth="25px"
    android:minHeight="25px">
    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:theme="@style/ToolBarColor"
        android:minHeight="?attr/actionBarSize"
        app:navigationIcon="@drawable/logo"
        android:background="#333333"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <de.hdodenhof.circleimageview.CircleImageView
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:id="@+id/circleImageView1"
        android:src="@drawable/james2"
        android:layout_gravity="center"
        android:layout_marginTop="10dp"
        app:civ_border_color="#fff2854b"
        app:civ_border_width="2dp"
        app:civ_fill_color="#00000000" />
    <RatingBar
        android:layout_height="wrap_content"
        android:id="@+id/ratingBar1"
        android:numStars="5"
        android:stepSize="0.5"
        android:isIndicator="true"
        android:layout_width="wrap_content"
        android:layout_gravity="center" />
    <ScrollView
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/scrollViewProfile">
        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:stretchColumns="1">
            <TextView
                android:text="James Kulon"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/txtProfileUserName"
                android:gravity="center"
                android:textSize="25sp"
                android:textStyle="bold"
                android:typeface="serif"
                android:textColor="#ffffff" />
            <TextView
                android:text="Bass Guitar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/txtProfileInstrument"
                android:gravity="center"
                android:textColor="#f2854b"
                android:textSize="16sp"
                android:textStyle="bold"
                android:typeface="serif" />
            <Button
                android:text="Edit Profile"
                android:id="@+id/btnEditProfile"
                android:background="@android:color/transparent"
                android:textColor="#f2854b"
                android:textStyle="bold"
                android:typeface="serif"
                android:layout_width="match_parent"
                android:layout_height="21.0dp"
                android:gravity="right"
                android:textSize="12sp" />
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="24.5dp"
                android:id="@+id/linearLayout1"
                android:background="#333333"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp">
                <TextView
                    android:text="Email:"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:id="@+id/txtProfileEmail"
                    android:textColor="#f2854b"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    android:typeface="serif"
                    android:layout_marginLeft="10dp" />
                <TextView
                    android:text="j@awesomemail.com"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:id="@+id/txtProfileEmail"
                    android:gravity="center"
                    android:layout_marginLeft="10dp"
                    android:textColor="#ffffff"
                    android:textSize="16sp"
                    android:typeface="serif" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearLayout4"
                android:background="#333333"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp">
                <TextView
                    android:text="About Me: "
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:id="@+id/txtProfileAboutMe"
                    android:layout_marginLeft="10dp"
                    android:textColor="#f2854b"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    android:typeface="serif" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearLayout5"
                android:background="#333333"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp">
                <TextView
                    android:text= "I am a bassist that has a diverse amount of experience in quite a few generes of music.  Heavily influenced by Rush, Boston, Yes with a lot of funk and Jazz influence as well.  In addition to the Bass and backing Vocals, I also play Guitar and Keyboards"
                    android:id="@+id/txtProfileBio"
                    android:textColor="#ffffff"
                    android:textSize="16sp"
                    android:typeface="serif"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dp" />
            </LinearLayout>
            <Switch
                android:text="                             Are You Available to Sub?"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:checked="true"
                android:clickable="true"
                android:id="@+id/profileSwitch"
                android:textOn="Available for Work"
                android:textOff="Not Available"
                android:textColor="#f2854b"
                android:textSize="15dp"
                android:textStyle="bold"
                android:typeface="serif" />
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="39.5dp"
                android:id="@+id/linearLayout6"
                android:gravity="center"
                android:layout_marginTop="30dp">
                <Button
                    android:text="Search"
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:id="@+id/btnSearch"
                    android:background="@drawable/Login_Style_Button"
                    android:textColor="#ffffff"
                    android:textStyle="bold"
                    android:typeface="serif"
                    android:gravity="center" />
            </LinearLayout>
        </TableLayout>
    </ScrollView>
</LinearLayout>